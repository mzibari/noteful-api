    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "xss": "^1.0.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
  }
